page{
  background-color: #f9f9f9;
}
/*轮播图*/
swiper{
  width: 100%;
  height: 30vh;
}

/*菜单项*/
.navigation_menu_group{
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 30rpx;
}
.navigation_menu_tabs{
  width: 94%;
  background-color: #fff;
  border-radius: 20rpx;
}
.navigation_menu_content{
  display: flex;
  flex-wrap: wrap;
  padding: 20rpx;
}
.navigation_menu_item{
  margin: 16rpx 20rpx;
  width: 120rpx;
  height: 160rpx;
}